{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:area",
            "@type": [
                "forest:QuantitativeProperty"
            ],
            "rdfs:comment": "Total area of the plantation.",
            "rdfs:label": "area",
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            },
            "forest:domainIncludes": [
                {
                    "@id": "forest:PlantationSites"
                }
            ],
            "forest:rangeIncludes": [
                {
                    "@id": "forest:Number"
                },
                {
                    "@id": "forest:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "sampleSizeValue"
            }
        }
    ]
}