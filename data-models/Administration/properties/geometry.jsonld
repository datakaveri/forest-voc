{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:geometry",
            "@type": [
                "forest:GeoProperty"
            ],
            "rdfs:comment": "Geographic object to represent areas as closed rings using longitude and latitude coordinates, where all points are listed in order, and the first and last must match for each ring.",
            "rdfs:label": "geometry",
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            },
            "forest:domainIncludes": [
                {
                    "@id": "forest:PlantationSites"
                }
            ],
            "forest:rangeIncludes": [
                {
                    "@id": "forest:Polygon"
                },
                {
                    "@id": "forest:ValueDescriptor"
                }
            ]
        }
    ]
}
