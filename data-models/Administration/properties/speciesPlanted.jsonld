{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:speciesPlanted",
            "@type": [
                "forest:StructuredProperty"
            ],
            "rdfs:comment": "Array of species planted at this plantation site with their respective counts. Each item must have 'code' (species identifier) and 'count' (number planted) fields.",
            "rdfs:label": "speciesPlanted",
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            },
            "forest:domainIncludes": [
                {
                    "@id": "forest:PlantationSites"
                }
            ],
            "forest:rangeIncludes": [
                {
                    "@id": "forest:DataFieldListValue"
                }
            ],
            "forest:dataSchema": {
                "@type": "forest:DataDescriptor",
                "forest:dataDescriptorName": "SpeciesPlantedList",
                "forest:dataDescriptorLabel": "Species Planted List",
                "forest:requiredProperty": [
                    {
                        "@id": "forest:code"
                    },
                    {
                        "@id": "forest:count"
                    }
                ],
                "forest:dataFieldListValue": [
                    {
                        "@id": "forest:code",
                        "forest:dataDescriptorName": "Species Code",
                        "forest:dataDescriptorLabel": "Species Code",
                        "forest:rangeIncludes": [
                            {
                                "@id": "forest:Text"
                            }
                        ]
                    },
                    {
                        "@id": "forest:count",
                        "forest:dataDescriptorName": "Plant Count",
                        "forest:dataDescriptorLabel": "Plant Count",
                        "forest:rangeIncludes": [
                            {
                                "@id": "forest:Number"
                            }
                        ]
                    }
                ]
            }
        }
    ]
}
