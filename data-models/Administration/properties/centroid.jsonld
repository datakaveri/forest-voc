{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:centroid",
            "@type": [
                "forest:GeoProperty"
            ],
            "rdfs:comment": "Geographic centroid coordinate of the plantation polygon.",
            "rdfs:label": "centroid",
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            },
            "forest:domainIncludes": [
                {
                    "@id": "forest:PlantationSites"
                }
            ],
            "forest:rangeIncludes": [
                {
                    "@id": "forest:Point"
                },
                {
                    "@id": "forest:GeoJSONGeometry"
                },
                {
                    "@id": "forest:Place"
                },
                {
                    "@id": "forest:Text"
                },
                {
                    "@id": "forest:ValueDescriptor"
                }
            ]
        }
    ]
}
