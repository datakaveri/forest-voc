{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "iudx": "https://voc.iudx.org.in/",
    "adex": "https://agrijson.org/",
    "forest": "https://voc.forest-stack.iudx.io/"
  },
  "@graph": [
    {
      "@id": "forest:country",
      "@type": [
        "forest:TextProperty"
      ],
      "rdfs:comment": "The name of the country in which the census was conducted. Use ISO 3166-1 country codes when possible (e.g., 'IN' for India)",
      "rdfs:label": "country",
      "rdfs:isDefinedBy": {
        "@id": "forest:"
      },
      "forest:domainIncludes": [
        {
          "@id": "forest:WildlifeCensus"
        },
        {
          "@id": "forest:HumanWildlifeConflict"
        }
      ],
      "forest:rangeIncludes": [
        {
          "@id": "forest:Text"
        }
      ]
    }
  ]
}