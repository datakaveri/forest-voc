{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:cropArea",
            "@type": [
                "forest:QuantitativeProperty"
            ],
            "rdfs:comment": "Area of the agricultural land used for cultivation, i.e, the arable land area.",
            "rdfs:label": "cropArea",
            "adex:domainIncludes": [
                {
                    "@id": "forest:LandParcel"
                },
                {
                    "@id": "forest:CropCondition"
                },
                {
                    "@id": "forest:Irrigation"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@id": "forest:Number"
                },
                {
                    "@id": "forest:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            }
        }
    ]
}