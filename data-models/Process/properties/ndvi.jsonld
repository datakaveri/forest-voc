{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:ndvi",
            "@type": [
                "forest:TextProperty"
            ],
            "rdfs:comment": "The value of Normalized Differential Vegetation Index (NDVI) which is a metric for quantifying the health and density of vegetation corresponding to this observation. The NDVI values range from -1 to +1.",
            "rdfs:label": "ndvi",
            "adex:domainIncludes": [
                {
                    "@id": "forest:CropCondition"
                },
                {
                    "@id": "forest:HyperLocalAdvisory"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@container": "@list",
                    "@id": "forest:Text"
                },
                {
                    "@id": "forest:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            }
        }
    ]
}