{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:ghgName",
            "@type": [
                "forest:TextProperty"
            ],
            "rdfs:comment": "The name(s) of greenhouse gas(es) emitted during the reporting period. The presence of multiple gases is represented by enclosing them in a list.",
            "rdfs:label": "ghgName",
            "rdfs:isDefinedBy": {
                "@id": "forest:"
            },
            "adex:domainIncludes": [
                {
                    "@id": "forest:GhgReport"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@container": "@list",
                    "@id": "forest:Text"
                },
                {
                    "@id": "forest:ValueDescriptor"
                }
            ]
        }
    ]
}