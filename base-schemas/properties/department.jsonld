{
    "@context": {
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:department",
            "@type": [
                "forest:TextProperty"
            ],
            "rdfs:comment": "Division or section of the organization corresponding to this observation. Eg. Health, Transport.",
            "rdfs:label": "department",
            "adex:domainIncludes": [
                {
                    "@id": "forest:Model"
                },
                {
                    "@id": "forest:Resource"
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@id": "forest:Text"
                }
            ]
        }
    ]
}