{
    "@context": {
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "adex": "https://agrijson.org/",
        "forest": "https://voc.forest-stack.iudx.io/"
    },
    "@graph": [
        {
            "@id": "forest:tags",
            "@type": [
                "forest:TextProperty"
            ],
            "rdfs:comment": "Comma separated discovery tags associated with an ADEXEntity (resource , resource Group)",
            "rdfs:label": "tags",
            "adex:domainIncludes": [
                {
                    "@id": "forest:Resource",
                    "adex:requiredProperty": true
                },
                {
                    "@id": "forest:ResourceGroup",
                    "adex:requiredProperty": true
                },
                {
                    "@id": "forest:Model",
                    "adex:requiredProperty": true
                }
            ],
            "adex:rangeIncludes": [
                {
                    "@container": "@list",
                    "@id": "forest:Text"
                }
            ]
        }
    ]
}